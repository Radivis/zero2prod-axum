  "query": "\n        SELECT \n            bp.id,\n            bp.title,\n            bp.content,\n            bp.status,\n            bp.author_id,\n            u.username as author_username,\n            bp.created_at,\n            bp.updated_at\n        FROM blog_posts bp\n        INNER JOIN users u ON bp.author_id = u.user_id\n        ORDER BY bp.created_at DESC\n        ",
